
rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    
    // Allow read and write access to all documents for any authenticated user.
    // This is useful for rapid development but should be replaced with more
    // secure rules before launching to production.
    match /{document=**} {
      allow read, write: if request.auth != null;
    }
  }
}
